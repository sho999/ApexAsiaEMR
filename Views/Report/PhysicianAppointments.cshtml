@model List<ApexAsiaDAL.viewAppointment>
@{
    ViewBag.Title = "Physician Appointments";
    
}
<table width="100%">
    <tr>
        <td valign="top" align="left" class="pageHeading" width="50%">
            @Html.Label("Physician's Appointments")
        </td>
        <td align="left" width="50%">
            <img src="../../Images/appointment-icon.png" alt="appointment icon" width="60" height="60" />
        </td>
    </tr>
</table>

<div id="physicianDetails" class="k-block k-success-colored" style=" width:98%; margin:5px; text-align: center; font-weight:bold;">
    @ViewBag.PhysicianTitle.Prefix  @ViewBag.PhysicianTitle.Name [@ViewBag.PhysicianTitle.Degree]
    <br />
    @ViewBag.PhysicianTitle.Title1<br />
    @ViewBag.PhysicianTitle.Title2<br />
    @ViewBag.PhysicianTitle.Title3<br />
   
</div>

@Html.Partial("_appointmentTablePartial")
<p>&nbsp;</p>



